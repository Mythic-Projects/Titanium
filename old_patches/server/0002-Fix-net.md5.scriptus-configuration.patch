 														<format>-CB.%s</format>
-														<scmDirectory>../base/Paper/CraftBukkit</scmDirectory>
+														<scmDirectory>../base/SportPaper/base/Paper/CraftBukkit</scmDirectory>
 														<descriptionProperty>craftbukkit.desc</descriptionProperty>
 												</configuration>
 												<phase>initialize</phase>
@@ -182,7 +194,7 @@
                         <manifestEntries>
                             <Main-Class>org.bukkit.craftbukkit.Main</Main-Class>
                             <Implementation-Title>CraftBukkit</Implementation-Title>
-                            <Implementation-Version>${sportpaper.desc}${paper.desc}${spigot.desc}${craftbukkit.desc}</Implementation-Version>
+                            <Implementation-Version>${titanium.desc} ${sportpaper.desc} ${paper.desc}${spigot.desc}${craftbukkit.desc}</Implementation-Version>
                             <Implementation-Vendor>Bukkit Team</Implementation-Vendor>
                             <Specification-Title>Bukkit</Specification-Title>
                             <Specification-Version>${api.version}</Specification-Version>
